{
  "expo": {
    "name": "Aji Salit",
    "slug": "ajisalit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aji-salit",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*",
      "assets/fonts/*"
    ],
    "splash": {
      "image": "./assets/images/splashs.png",
      "resizeMode": "cover",
      "backgroundColor": "#fff"
    },
    "ios": {
      "googleServicesFile": "./google-services.json",
      "supportsTablet": false,
      "bundleIdentifier": "com.ajisalit.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splashs.png",
        "backgroundColor": "#ffffff",
        "userInterfaceStyle": "light"
      },
      "package": "com.ajisalit.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "config": {
        "usesCleartextTraffic": true
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logo-notification.jpg"
        }
      ]
    ],
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "46ec5070-4c89-4c34-9dbe-7350c1a1d4c5"
      }
    },
    "owner": "tttesttt12"
  }
}

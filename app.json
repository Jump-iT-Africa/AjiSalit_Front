{
  "expo": {
    "name": "Aji Salit",
    "slug": "ajisalit",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "aji-salit",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*", "assets/fonts/*"],
    "splash": {
      "image": "./assets/images/splashs.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "googleServicesFile": "./google-services.json",
      "supportsTablet": false,
      "bundleIdentifier": "app.ajisalit.com",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": ["remote-notification"],
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes"
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splashs.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.ajisalit.com",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/logooo.png",
          "color": "#ffffff"
        }
      ]
    ],
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "58578925-3025-4070-823b-d44a161b2c53"
      }
    },
    "owner": "expobiling"
  }
}

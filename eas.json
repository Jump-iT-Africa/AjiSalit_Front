{
  "cli": {
    "version": ">= 15.0.10",
    "requireCommit": true,
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "cache": {
        "disabled": false
      },
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "store",
      "cache": {
        "disabled": false
      },
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "autoIncrement": true,
      "cache": {
        "disabled": false
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6744866152",
        "appleTeamId": "STJUA634KR"
      }
    },
    "preview": {
      "ios": {
        "ascAppId": "6744866152",
        "appleTeamId": "STJUA634KR"
      }
    }
  }
}
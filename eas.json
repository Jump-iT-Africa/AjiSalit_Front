{
  "cli": {
    "version": ">= 15.0.10",
    "requireCommit": true,
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "cache": {
        "disabled": false
      }
    },
    "preview": {
      "distribution": "internal",
      "cache": {
        "disabled": false
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      }
    },
    "production": {
      "autoIncrement": true,
      "cache": {
        "disabled": false
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6744866152",
        "appleTeamId": "STJUA634KR",
        "ascApiKeyId": "${APPLE_API_KEY_ID}",
        "ascApiKeyIssuerId": "${APPLE_API_ISSUER_ID}"
      }
    },
    "preview": {
      "ios": {
        "ascAppId": "6744866152",
        "appleTeamId": "STJUA634KR",
        "ascApiKeyId": "${APPLE_API_KEY_ID}",
        "ascApiKeyIssuerId": "${APPLE_API_ISSUER_ID}"
      }
    }
  }
}